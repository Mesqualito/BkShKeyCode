<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ItemReference.
    -->
    <changeSet id="20191008094047-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="reference_id"
                                 baseTableName="item_reference"
                                 constraintName="fk_item_reference_reference_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rank"/>

        <addForeignKeyConstraint baseColumnNames="item_id"
                                 baseTableName="item_reference"
                                 constraintName="fk_item_reference_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="item"/>

    </changeSet>
</databaseChangeLog>
